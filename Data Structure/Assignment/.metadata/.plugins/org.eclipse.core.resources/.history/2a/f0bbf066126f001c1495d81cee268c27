#ifndef STUDENTDB_H_
#define STUDENTDB_H_

#include "Linkedlist.h"
#include "string.h"
// student data
typedef struct {
	uint8_t day;
	uint8_t month;
	uint32_t year;
}S_Birth_date;
typedef struct {
	uint8_t name[40];
}S_student;




//Student DataBase Attributes
typedef struct {
	uint8_t day;
	uint8_t month;
	uint32_t year;

} S_birth_date ;

typedef struct
{
	uint8_t name[40];
	uint8_t Id;
	float height;
	S_birth_date birthday;

} S_student;
